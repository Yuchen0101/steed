{
  "expo": {
    "scheme": "housepunt",
    "name": "HousePunt",
    "slug": "HousePunt",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a1a2f"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "steed-intel.com",
      "buildNumber":"8",
      "infoPlist": {
        "NSLocationUsageDescription": "Turn on location services allows us to show nearby properties to your current location.",
        "NSPhotoLibraryUsageDescription": "Allow access to photos to upload photos from library."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
